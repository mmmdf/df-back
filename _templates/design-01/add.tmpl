<div class="container-fluid">
	<div class="row">
		<div class="col-lg-12">
			<div class="container-fluid">
				<div class="row py-3">
					<div class="offset-md-3 col-md-6 d-flex justify-content-md-center">
						<h2>Add new report</h2>
					</div>
				</div>
			</div>
			<div class="container-fluid p-2 bg-light">
				<div class="row mx-0">
					<div class="col-md-12 p-2 bg-white">
						<div class="row">
							<div class="col-xs-3 col-sm-3 col-md-3 col-lg-3">
								<div class="form-group">
									<label for="name">Name</label>
									<input type="text" id="name" class="form-control">
								</div>
							</div>
							<div class="col-xs-3 col-sm-3 col-md-3 col-lg-3">
								<div class="form-group">
									<label for="surname">Surname</label>
									<input type="text" id="surname" class="form-control">
								</div>
							</div>
							<div class="col-xs-3 col-sm-3 col-md-3 col-lg-3">
								<div class="form-group">
									<label for="email">Email</label>
									<input type="text" id="email" class="form-control">
								</div>
							</div>
							<div class="col-xs-3 col-sm-3 col-md-3 col-lg-3">
								<div class="form-group">
									<label for="mobile">Mobile</label>
									<input type="text" id="mobile" class="form-control">
								</div>
							</div>
						</div>
					</div>
				</div>
				<div class="row mx-0 my-2">
					<div class="col-md-3 pl-0">
						<div class="card p-2 border-0 rounded-0">
							<div class="form-group">
								<label>Service</label>

								<select id="type" class="form-control">
									<option value="1" selected="selected">Meet &amp; Greet</option>
									<option value="2">Park &amp; Ride</option>
								</select>
							</div>

							<div class="form-group">
								<label>Airport</label>

								<select id="airport" class="form-control">
									<option value="1" selected="selected">Heathrow</option>
									<option value="7">Birmingham</option>
									<option value="3">Luton</option>
								</select>
							</div>
						</div>
					</div>
					<div class="col-md-6 px-0">
						<div class="card py-2 px-3 border-0 rounded-0">
							<div class="row">
								<div class="col-md-6">
									<div class="form-group">
										<label for="dateStart">Leaving on</label>
										<input type="text" id="dateStart" class="form-control">
									</div>
								</div>
								<div class="col-md-6">
									<div class="form-group">
										<label for="timeStart">Leaving Time</label>
										<input type="text" id="timeStart" class="form-control">
									</div>
								</div>
								<div class="col-md-6">
									<div class="form-group">
										<label for="dateEnd">Returning on</label>
										<input type="text" id="dateEnd" class="form-control">
									</div>
								</div>
								<div class="col-md-6">
									<div class="form-group">
										<label for="timeEnd">Returning Time</label>
										<input type="text" id="timeEnd" class="form-control">
									</div>
								</div>
							</div>
						</div>
					</div>
					<div class="col-md-3 pr-0">
						<div class="card p-2 border-0 rounded-0">
							<div class="col-xs-6"> </div>
							<div class="col-xs-6">
								<div class="form-group">
									<label for="price">price</label>
									<input type="text" id="price" class="form-control">
								</div>
								<div class="label label-default">Full price: 0</div>
							</div>
						</div>
					</div>
				</div>

				<div class="row mx-0 my-2">
					<div class="col-md-8 pl-0">
						<div class="card p-2 border-0 rounded-0">
							<div class="row">
								<div class="col-md-4">
									<div class="form-group">
										<label for="carReg">Car Reg</label>
										<input type="text" id="carReg" class="form-control">
									</div>
								</div>
								<div class="col-md-4">
									<div class="form-group">
										<label for="carModel">Car Model</label>
										<input type="text" id="carModel" class="form-control">
									</div>
								</div>
								<div class="col-md-4">
									<div class="form-group">
										<label for="carColor">Car Colour</label>
										<input type="text" id="carColor" class="form-control">
									</div>
								</div>
							</div>
						</div>
						<div class="card p-2 border-0 rounded-0">
							<div class="row">
								<div class="col-md-4">
									<div class="form-group">
										<label for="flightEnd">Return flight</label>
										<input type="text" id="flightEnd" class="form-control">
									</div>
								</div>
								<div class="col-md-4">
									<div class="form-group">
										<label for="terminalStart">Leaving terminal</label>

										<select class="form-control">
											<option value="" selected="selected"></option>
											<option value="1" class="">1</option>
											<option value="2" class="">2</option>
											<option value="3" class="">3</option>
											<option value="4" class="">4</option>
											<option value="5" class="">5</option>
											<option value="south">south</option>
											<option value="north">north</option>
										</select>
									</div>
								</div>
								<div class="col-md-4">
									<div class="form-group">
										<label for="terminalEnd">Return terminal </label>

										<select class="form-control">
											<option value="" selected="selected"></option>
											<option value="1" class="">1</option>
											<option value="2" class="">2</option>
											<option value="3" class="">3</option>
											<option value="4" class="">4</option>
											<option value="5" class="">5</option>
											<option value="south">south</option>
											<option value="north">north</option>
										</select>
									</div>
								</div>
							</div>
						</div>
					</div>
					<div class="col-md-4 p-0">
						<div class="card py-2 px-3 border-0 rounded-0">
							<div class="form-group">
								<label for="agent">Consolidator</label>
								<select class="form-control">
									<option value="1" selected="selected">Drivefly</option>
									<option value="32">ICPR</option>
									<option value="7">NEW</option>
									<option value="4">Skypark</option>
									<option value="16">CHR</option>
									<option value="6">L4P</option>
									<option value="24">EX</option>
									<option value="3">APH</option>
									<option value="12">P-P</option>
									<option value="25">SPF</option>
									<option value="17">HD</option>
									<option value="2">FHR</option>
									<option value="10">UKPF</option>
									<option value="15">A2B</option>
									<option value="11">P&amp;G</option>
									<option value="28">Hilton</option>
									<option value="27">Ibis</option>
									<option value="29">P&amp;M</option>
									<option value="30">WOW</option>
									<option value="31">Groupon</option>
									<option value="34">ILAP</option>
									<option value="36">ILMAG</option>
									<option value="35">TT</option>
									<option value="22">Search 4 Parking</option>
									<option value="38">Sky Parking Services</option>
								</select>
							</div>

							<div class="form-group">
								<label for="ref">Reference</label>
								<input type="text" id="ref" class="form-control">
								<small>Leave empty to autogenerate</small>
							</div>
							<div class="form-group">
								<label for="product">Product</label>
								<input type="text" id="product" class="form-control">
							</div>
						</div>

					</div>
				</div>
				<div class="row mx-0 my-2 d-flex align-items-end">
					<div class="col-md-6 pl-0">
						<div class="card py-2 px-3 border-0 rounded-0">
							<div class="form-group">
								<label for="comment">comment</label>
								<textarea maxlength="255" rows="5" class="form-control"></textarea>
							</div>
						</div>
					</div>
					<div class="col-md-6 pr-0">
						<button type="button" class="btn btn-large btn-block btn-secondary">Add and open payment page</button>
						<button type="button" class="btn btn-large btn-block btn-secondary">Add as Paid</button>
						<button type="button" class="btn btn-large btn-block btn-secondary">Add as Paid and Send Confirmation</button>
					</div>
				</div>
			</div>
		</div>
	</div>
<div>